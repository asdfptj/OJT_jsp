<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>Chap04_JDBC_tesc</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>GetEmpList</servlet-name>
		<servlet-class>com.company.emp.GetEmpList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetEmpList</servlet-name>
		<url-pattern>/GetEmpList</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GetEmp</servlet-name>
		<servlet-class>com.company.emp.GetEmp</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetEmp</servlet-name>
		<url-pattern>/GetEmp</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteEmp</servlet-name>
		<servlet-class>com.company.emp.DeleteEmp</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteEmp</servlet-name>
		<url-pattern>/DeleteEmp</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>InsertEmp</servlet-name>
		<servlet-class>com.company.emp.InsertEmp</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InsertEmp</servlet-name>
		<url-pattern>/InsertEmp</url-pattern>
	</servlet-mapping>



</web-app>